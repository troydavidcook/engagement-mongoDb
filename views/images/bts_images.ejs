<% include ../partials/header %>

    <div class="ui container main">
        <div class="ui container segment">
            <h3 id="scroll-down-text">Scroll down to see how it happened!</h3>

            <% bts_image.forEach((photo) => { %>
                <a href="/images/<%= photo._id %>"><img class="ui centered image bordered massive" src="<%= photo.url %>" alt=""></a>
                <h4><a id="a-leave-comment" href="/images/<%= photo._id %>">Leave a comment</a></h4>
                <hr class="style-two">
            <% }); %>
            <a href="/images"><h3 id="behind-scenes-text">Back to the Beginning!</h3></a>
        </div>
    </div>

<% include ../partials/footer %>